<?xml version="1.0" encoding="utf-8"?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <time type="FastUTC"/>
    <targets>
        <target name="console" xsi:type="ColoredConsole" layout="[${uppercase:${level}}][${logger:shortName=true}] ${message} ${exception:format=tostring}"/>
    </targets>
    <rules>
        <logger name="*" writeTo="console"/>
    </rules>
</nlog>
